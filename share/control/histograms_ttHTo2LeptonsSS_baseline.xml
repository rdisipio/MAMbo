<histograms>
  <TH1F name="cutflow" title="CutFlow" nbins="6" xmin="-0.5" xmax="5.5" />

  <TH1F name="el_n"   title="No. of electrons" nbins="6"  xmin="-0.5" xmax="5.5" />
  <TH1F name="el_pt"  title="Electron p_{T} [GeV]"           nbins="200" xmin="0."   xmax="1000." />
  <TH1F name="el_eta" title="Electron pseudorapidity #eta"   nbins="20" xmin="-3."  xmax="3." />
  <TH1F name="el_phi" title="Electron azimuthal angle #phi"  nbins="32" xmin="-3.2"   xmax="3.2" />
  <TH1F name="el_E"   title="Electron energy [GeV]"          nbins="200" xmin="0."   xmax="1000." />

  <TH1F name="mu_n"   title="No. of muons" nbins="5"  xmin="-0.5" xmax="4.5" />
