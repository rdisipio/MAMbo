  <TH1F name="jet_n"   title="No. of Jets" nbins="15"  xmin="-0.5" xmax="14.5" />
  <TH1F name="jet_pt"  title="Jet p_{T} [GeV]"           nbins="200" xmin="0."   xmax="1000." />
  <TH1F name="jet_eta" title="Jet pseudorapidity #eta"   nbins="50" xmin="-2.5"  xmax="2.5" />
