 <histograms>
<!-- "beforeCuts", "0el", "2mu", "Zm", "ZpT", "1fj", "dPhi_Zfj" -->
    <Level> <!--Level--> 
        <Type name="reco" />                 <!-- Passed: reco-->
    </Level>
    <Level> <!--Region-->
        <!--Type name="4j1b" /-->
        <Type name="cutflow" />
    </Level>
    <Level> <!--Particle-->
        <Type name="beforeCuts" inFolder="cutflow" />
        <Type name="0el" inFolder="cutflow" />
        <Type name="2mu" inFolder="cutflow" />
        <Type name="Zm" inFolder="cutflow" />
        <Type name="ZpT" inFolder="cutflow" />
        <Type name="1fj" inFolder="cutflow" />
        <Type name="dPhi_Zfj" inFolder="cutflow" />
    </Level>
    <Variables>
        <Variable name="el_pt" title="Electron p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="el_n"   title="No. of Electrons" folder="cutflow">
            <Bin id="0" nbins="4" xmin="-0.5" xmax="3.5" />
        </Variable>
        <Variable name="el_eta" title="Electron pseudorapidity" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" />
        </Variable>
        <Variable name="el_phi" title="Electron azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="mu_pt" title="Muon p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="mu_n"   title="No. of Muons" folder="cutflow">
            <Bin id="0" nbins="20" xmin="-0.5" xmax="19.5" />
        </Variable>
        <Variable name="mu_eta" title="Muon pseudorapidity" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" />
       	</Variable>
        <Variable name="mu_phi" title="Muon azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="jet_pt" title="Jet p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="jet_n"   title="No. of Jets" folder="cutflow">
            <Bin id="0" nbins="20" xmin="-0.5" xmax="19.5" />
        </Variable>
        <Variable name="jet_eta" title="Jet pseudorapidity" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" />
       	</Variable>
        <Variable name="jet_phi" title="Jet azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>    
        <Variable name="jet_m" title="Jet mass [GeV]" folder="cutflow" >
            <Bin id="0" nbins="60" xmin="0" xmax="300" />
        </Variable>
        <Variable name="bjet_pt" title="b-Jet p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="bjet_n"   title="No. of b-Jets" folder="cutflow">
            <Bin id="0" nbins="6" xmin="-0.5" xmax="5.5" />
        </Variable>
        <Variable name="bjet_eta" title="b-Jet pseudorapidity" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" />
       	</Variable>
        <Variable name="bjet_phi" title="b-Jet azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="bjet_m" title="b-Jet mass [GeV]" folder="cutflow" >
            <Bin id="0" nbins="60" xmin="0" xmax="300" />
       	</Variable>
        <Variable name="fjet_pt" title="Fat-Jet p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="fjet_n"   title="No. of Fat-Jets" folder="cutflow">
            <Bin id="0" nbins="6" xmin="-0.5" xmax="5.5" />
        </Variable>
        <Variable name="fjet_eta" title="Fat-Jet pseudorapidity" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" />
        </Variable>
        <Variable name="fjet_phi" title="Fat-Jet azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="fjet_m" title="Fat-Jet mass [GeV]" folder="cutflow" >
            <Bin id="0" nbins="60" xmin="0" xmax="300" />
        </Variable>
        <Variable name="Z_m" title="Z boson mass [GeV]" folder="cutflow" >
            <Bin id="0" nbins="60" xmin="60" xmax="120" />
        </Variable>
        <Variable name="Z_pt" title="Z boson p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="50" xmin="0" xmax="500" />
        </Variable>
        <Variable name="dPhi" title="#Delta#phi" folder="cutflow" >
            <Bin id="0" nbins="32" xmin="0." xmax="3.14" />
        </Variable>
    </Variables>
    <Histos>
       <TH1F variable="el_n"     binId="0"/>
       <TH1F variable="el_pt"    binId="0"/>
       <TH1F variable="el_eta"   binId="0"/>
       <TH1F variable="el_phi"   binId="0"/>
       <TH1F variable="mu_n"     binId="0"/>
       <TH1F variable="mu_pt"    binId="0"/>
       <TH1F variable="mu_eta"   binId="0"/>
       <TH1F variable="mu_phi"   binId="0"/>
       <TH1F variable="jet_n"    binId="0"/>
       <TH1F variable="jet_pt"   binId="0"/>
       <TH1F variable="jet_eta"  binId="0"/>
       <TH1F variable="jet_phi"  binId="0"/>
       <TH1F variable="jet_m"    binId="0"/>
       <TH1F variable="bjet_n"   binId="0"/>
       <TH1F variable="bjet_pt"  binId="0"/>
       <TH1F variable="bjet_eta" binId="0"/>
       <TH1F variable="bjet_phi" binId="0"/>
       <TH1F variable="bjet_m"   binId="0"/>
       <TH1F variable="fjet_n"   binId="0"/>
       <TH1F variable="fjet_pt"  binId="0"/>
       <TH1F variable="fjet_eta" binId="0"/>
       <TH1F variable="fjet_phi" binId="0"/>
       <TH1F variable="fjet_m"   binId="0"/>

       <TH1F variable="Z_m"      binId="0" />
       <TH1F variable="Z_pt"     binId="0" />
       <TH1F variable="dPhi"     binId="0" />

       <TH2F variableX="Z_pt" binIdX="0" variableY="dPhi" binIdY="0" />
<!--
        <TH2F variableX="dPhi_ttbar" binIdX="0" variableY="Salam_ttbar" binIdY="0" folder="4j2b" particle="difference" />
-->
    </Histos>
</histograms>
