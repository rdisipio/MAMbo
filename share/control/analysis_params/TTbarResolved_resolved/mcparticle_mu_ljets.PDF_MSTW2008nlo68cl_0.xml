<analysis name="TTbarResolved">
  <ntuple>TopMiniSLResolvedParticles</ntuple>
  <branches>control/branch_list_TopMiniSLResolved.txt</branches>
  <tree>particles</tree>
  <histograms>control/histograms_TTbarResolved_baseline.xml</histograms>
  <cutflow>TTbarResolvedParticleLevel</cutflow>
  <channel>muon</channel>
  <parameters>
    <param name="pTmin"             type="numeric" value="20." />
    <param name="etaMax"            type="numeric" value="2.5" />
    <param name="isMCSignal"        type="flag"    value="1" />
    <param name="isRealData"        type="flag"    value="0" />
    <param name="isWjets"      	    type="flag"    value="0" />
    <param name="isQCD"             type="flag"    value="0" />
    <param name="isDilepton"   	    type="flag"    value="0" />
    <param name="scale_syst"        type="string"  value="PDF:MSTW2008nlo68cl:0" />
  </parameters>
</analysis>
<!-- NB: signal=ljets so dileptons are discarded -->
