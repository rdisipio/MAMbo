<histograms>
    <Level> <!--Level-->
        <Type name="reco" />
        <Type name="particle" />
        <Type name="parton" />
        <Type name="particle_not_reco" />
        <Type name="reco_and_particle" /> 
        <Type name="reco_not_particle" />
        <Type name="matched" />
    </Level>
    <Level> <!--Region-->
        <!--Type name="4j1b" /-->
        <Type name="4j2b" />
        <Type name="cutflow" />
        <Type name="matching" inFolder="reco"/>
    </Level>
    <Level> <!--Particle-->
        <Type name="topL" inFolder="4j2b,matching"/>
        <Type name="topH" inFolder="4j2b,matching"/>
        <Type name="tt" inFolder="4j2b,matching"/>
        <Type name="difference" inFolder="4j2b"/>
        <Type name="beforeCuts" inFolder="cutflow"/>
        <Type name="4j1b" inFolder="cutflow"/>
        <Type name="afterCuts" inFolder="cutflow"/>        
    </Level>
    <Variables>
        <Variable name="pt" title="p_{T} [GeV]" >
            <Bin id="0" nbins="10" edges="0,45,90,120,150,180,220,260,300,350,1000" particle="top"/>
            <Bin id="1" nbins="11" edges="0,40,80,100,120,150,180,200,260,300,350,1000" particle="top"/>
            <Bin id="2" nbins="12" edges="0,40,75,90,110,135,160,180,220,260,300,350,1000" particle="top"/>
            <Bin id="3" nbins="9"  edges="0,50,100,140,180,220,260,300,350,1000" particle="top"/>            
            <Bin id="4" nbins="5"  edges="0,25,50,85,125,500" particle="tt"/>
        </Variable>

        <Variables name="ptdiff" title="p_{t} difference [GeV]" folder="difference">
            <Bin id="0" nbins="200" xmin="-100" xmax="100" />
        </Variables>

        <Variable name="eta" title="pseudorapidity #eta" folder="4j2b" > 
            <Bin id="0" nbins="25" xmin="-2.5" xmax="2.5"/>
        </Variable>
        <Variable name="phi"    title="azimuthal angle #phi" folder="4j2b" > 
            <Bin id="0" nbins="16" xmin="-3.2"   xmax="3.2"/>
        </Variable>
        <Variable name="E"      title="energy [GeV]" folder="4j2b" > 
            <Bin id="0" nbins="20" xmin="0." xmax="1500."/>
        </Variable>
        <Variable name="m"      title="mass [GeV]" folder="4j2b" > 
            <Bin id="0" nbins="50" xmin="0." xmax="600." particle="top"/>
            <Bin id="1" nbins="50" xmin="160." xmax="185." particle="top"/> 
            <Bin id="2" nbins="8" edges="225,400,500,600,700,850,1000,1250,2500" particle="tt" />
        </Variable>
        <Variable name="absrap" title="abs rapidity |y_{t}|" folder="4j2b" > 
            <Bin id="0" nbins="6" edges="0,0.4,0.8,1.1,1.4,1.8,2.5" particle="top"/>
	    <Bin id="1" nbins="10" edges="0,0.1,0.2,0.4,0.6,0.7,0.8,1.0,1.2,1.6,2.5" particle="tt"/>
        </Variable>
        <Variable name="mlb" title="Reco (b,l) invariant mass [GeV]" folder="4j2b" >
            <Bin nbins="50" xmin="0." xmax="300." />
        </Variable>
        <Variable name="lep_pt"  title="Lepton p_{T} [GeV]" folder="cutflow"> 
            <Bin id="0" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="lep_eta" title="Lepton pseudorapidity #eta" folder="cutflow">
            <Bin id="0" nbins="25" xmin="-2.5"  xmax="2.5" />
        </Variable>        
        <Variable name="lep_phi" title="Lepton azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="25" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="lep_E" title="Lepton energy [GeV]" folder="cutflow">
            <Bin id="0" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet_n"   title="No. of Jets" folder="cutflow">
            <Bin  nbins="15" xmin="-0.5" xmax="14.5" />
        </Variable>
        <Variable name="jet_pt"  title="Jet p_{T} [GeV]" folder="cutflow">
            <Bin nbins="200" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet1_pt"  title="Leading Jet p_{T} [GeV]" folder="cutflow">
            <Bin nbins="200" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet_eta" title="Jet pseudorapidity #eta" folder="cutflow">
            <Bin nbins="50" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="jet_phi" title="Jet azimuthal angle #phi" folder="cutflow">
            <Bin nbins="16" xmin="-3.2"   xmax="3.2" />
        </Variable>
        <Variable name="jet_E"   title="Jet energy [GeV]" folder="cutflow">
            <Bin nbins="200" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet_m"   title="Jet mass [GeV]" folder="cutflow">
            <Bin nbins="20" xmin="0."   xmax="100." />
        </Variable>
        <Variable name="fjet_n"   title="No. of Fat Jets" folder="cutflow">
            <Bin nbins="4" xmin="-0.5" xmax="3.5" />
        </Variable>
        <Variable name="fjet_pt"  title="Fat Jet p_{T} [GeV]" folder="cutflow">
            <Bin nbins="50" xmin="0."   xmax="1500." />
        </Variable>
        <Variable name="fjet_eta" title="Fat Jet pseudorapidity #eta" folder="cutflow">
            <Bin nbins="50" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="fjet_phi" title="Fat Jet azimuthal angle #phi" folder="cutflow">
            <Bin nbins="16" xmin="-3.2"   xmax="3.2" />
        </Variable>
        <Variable name="fjet_E"   title="Fat Jet energy [GeV]" folder="cutflow">
            <Bin nbins="50" xmin="0."   xmax="1500." />
        </Variable>
        <Variable name="fjet_m"   title="Fat Jet mass [GeV]" folder="cutflow">
            <Bin nbins="20" xmin="0."   xmax="400." />
        </Variable>
        <Variable name="fjet1_pt"  title="Leading Fat Jet p_{T} [GeV]" folder="cutflow">
            <Bin nbins="50" xmin="0."   xmax="1500." />
        </Variable>
        <Variable name="met_pt"  title="Missing Transverse Energy [GeV]"  folder="cutflow">
            <Bin nbins="100" xmin="0." xmax="200." />
        </Variable>
        <Variable name="mtw"     title="W boson Transverse Mass m_{T}^{W} [GeV]"  folder="cutflow">
            <Bin nbins="100" xmin="0" xmax="200" />
        </Variable>
        <Variable name="delta_R"  title="Matching #Delta R " folder="matching">
            <Bin nbins="20" xmin="0" xmax="5." />
        </Variable>
        <Variable name="delta_pt" title="Matching #Delta p_{T}" folder="matching">
            <Bin nbins="20" xmin="-200." xmax="200." />
        </Variable>
        <Variable name="delta_rapidity" title="Matching #Delta Y" folder="matching">
            <Bin nbins="20" xmin="-2." xmax="2." />
        </Variable>
        <Variable name="delta_phi" title="Matching #Delta #phi" folder="matching">
            <Bin nbins="20" xmin="-2." xmax="2." />
        </Variable>
        <Variable name="delta_m" title="Matching #Delta m" folder="matching">
            <Bin nbins="20" xmin="-200." xmax="200." />
        </Variable>


	<!-- JK top pair distributions -->
        <Variable name="z_ttbar" title="z_{t#bar{t}}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="0." xmax="3." />
        </Variable>
        <Variable name="Pout" title="p_{out}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="-300." xmax="300." />
        </Variable>
        <Variable name="Chi_ttbar" title="#chi_{out}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="14." xmax="3." />
        </Variable>
	<Variable name="Yboost" title="y_{boost}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="0." xmax="2." />
        </Variable>
	<Variable name="topLCosThetaStar" title="leptonic top cos #theta^{*};cos #theta*;" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="topHCosThetaStar" title="hadronic top cos #theta^{*};cos #theta*;" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="TopCosOpeningAngle" title="Tops cos #theta_{12};Top's cos #theta_{12};" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="Salam_ttbar" title="SalamDelta;[3*p_{T}^{(1)}-p_{T}^{(2)}] / H_{T}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="-1." xmax="3." />
        </Variable>
	<Variable name="dPhi_ttbar" title="DeltaPhi_ttbar;#Delta#phi_{t#bar{t}}" folder="4j2b" particle="difference" >
            <Bin nbins="100" xmin="0." xmax="3.14159" />
        </Variable>



  </Variables>        
  <Histos>
        <TH1F variable="ptdiff"/>
        <TH1F variable="pt" />
        <TH1F variable="eta" binId="0"/>
        <TH1F variable="phi" binId="0"/>
        <TH1F variable="E" binId="0"/>
        <TH1F variable="m" binId="0"/>
        <TH1F variable="absrap" binId="0"/>
        <TH1F variable="mlb" binId="0"/>
        <!--TH2F variableX="pt" binIdX="0" variableY="lep_pt" binIdY="0" /-->
        <TH1F variable="lep_pt" binId="0"/>
        <TH1F variable="lep_eta" binId="0"/>
        <TH1F variable="lep_phi" binId="0"/>                
        <TH1F variable="lep_E" binId="0"/>
        <TH1F variable="jet_n" binId="0"/>
        <TH1F variable="jet_pt" binId="0"/>
        <TH1F variable="jet_eta" binId="0"/>
        <TH1F variable="jet_phi" binId="0"/>                
        <TH1F variable="jet_E" binId="0"/>
        <TH1F variable="jet_m" binId="0"/>
        <TH1F variable="jet1_pt" binId="0"/>
        <TH1F variable="fjet_n" binId="0"/>
        <TH1F variable="fjet_pt" binId="0"/>
        <TH1F variable="fjet_eta" binId="0"/>
        <TH1F variable="fjet_phi" binId="0"/>                
        <TH1F variable="fjet_E" binId="0"/>
        <TH1F variable="fjet_m" binId="0"/>
        <TH1F variable="met_pt" binId="0"/>
        <TH1F variable="mtw" binId="0"/>
        <TH1F variable="delta_R" binId="0"/>
        <TH1F variable="delta_pt" binId="0"/>
        <TH1F variable="delta_rapidity" binId="0"/>
        <TH1F variable="delta_phi" binId="0"/>
        <TH1F variable="delta_m" binId="0"/>
        <Matrix variable="pt" binId="0"/>
        <Matrix variable="eta" binId="0"/>
        <Matrix variable="phi" binId="0"/>
        <Matrix variable="E" binId="0"/>
        <Matrix variable="m" binId="0"/>
        <Matrix variable="absrap" binId="0"/>

 
	<!-- JK top pair distributions -->
        <TH1F variable="z_ttbar" binId="0"/>
        <TH1F variable="Pout"  binId="0"/>
        <TH1F variable="Chi_ttbar"  binId="0"/>
	<TH1F variable="Yboost"  binId="0"/>
	<TH1F variable="topLCosThetaStar"  binId="0"/>
	<TH1F variable="topHCosThetaStar"  binId="0"/>
	<TH1F variable="TopCosOpeningAngle"  binId="0"/>
        <TH1F variable="Salam_ttbar"  binId="0"/>
	<TH1F variable="dPhi_ttbar"  binId="0"/>
        
      


    </Histos>    
    
  <!--TH1F name="vxp_nTracks"   title="No. of tracks per pv" nbins="20"  xmin="0" xmax="20" /-->
</histograms>
