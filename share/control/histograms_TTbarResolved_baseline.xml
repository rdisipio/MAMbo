<histograms>
    <Level> <!--Level-->
        <Type name="reco" />                 <!-- Passed: reco-->
        <Type name="particle" />             <!-- Passed: particle-->
        <Type name="parton" />
        <Type name="reco_and_particle" />    <!-- Passed: reco && particle-->
        <Type name="reco_not_particle" />    <!-- Passed: reco && !particle -->
        <Type name="matched" />              <!-- Passed: reco && particle && matched-->
    </Level>
    <Level> <!--Region-->
        <!--Type name="4j1b" /-->
        <Type name="4j2b" />
        <Type name="cutflow" />
        <Type name="matching" inFolder="reco"/>
    </Level>
    <Level> <!--Particle-->
        <Type name="topL" inFolder="4j2b,matching"/>
        <Type name="topH" inFolder="4j2b,matching"/>
        <Type name="tt" inFolder="4j2b,matching"/>
        <Type name="WL" inFolder="4j2b,matching"/>
        <Type name="WH" inFolder="4j2b,matching"/>
        <Type name="difference" inFolder="4j2b"/>
        <Type name="beforeCuts" inFolder="cutflow"/>
        <Type name="4j1b" inFolder="cutflow"/>
        <Type name="afterCuts" inFolder="cutflow"/>        
    </Level>
    <Variables>
        <Variable name="pt" title="p_{T} [GeV]" folder="4j2b" >
            <Bin id="0" nbins="10" edges="0,45,90,120,150,180,220,260,300,350,1000" particle="top"/>
            <Bin id="1" nbins="50" xmin="0" xmax="1000" particle="top"/>
            <Bin id="0" nbins="5"  edges="0,25,50,85,125,500" particle="tt"/>
            <Bin id="1" nbins="50" xmin="0" xmax="500"  particle="tt"/>
	    <Bin id="1" nbins="50" xmin="0" xmax="600" particle="WL"/>
	    <Bin id="1" nbins="50" xmin="0" xmax="600" particle="WH"/>
<!-- Not necessary: 
           <Bin id="1" nbins="11" edges="0,40,80,100,120,150,180,200,260,300,350,1000" particle="top"/>
            <Bin id="2" nbins="12" edges="0,40,75,90,110,135,160,180,220,260,300,350,1000" particle="top"/>
            <Bin id="3" nbins="9"  edges="0,50,100,140,180,220,260,300,350,1000" particle="top"/>            
-->
        </Variable>

        <Variables name="ptdiff" title="p_{t} difference [GeV]" folder="difference">
            <Bin id="0" nbins="200" xmin="-100" xmax="100" />
        </Variables>

        <Variable name="eta" title="pseudorapidity #eta" folder="4j2b" > 
            <Bin id="0" nbins="25" xmin="-2.5" xmax="2.5"/>
        </Variable>
        <Variable name="phi"    title="azimuthal angle #phi" folder="4j2b" > 
            <Bin id="0" nbins="16" xmin="-3.1416"   xmax="3.1416"/>
        </Variable>
        <Variable name="E"      title="energy [GeV]" folder="4j2b" > 
            <Bin id="0" nbins="20" xmin="0." xmax="1500."/>
        </Variable>
        <Variable name="m"      title="mass [GeV]" folder="4j2b" > 
            <Bin id="0" nbins="50" xmin="0." xmax="600." particle="top"/>
            <!-- <Bin id="1" nbins="50" xmin="160." xmax="185." particle="top"/> -->
            <Bin id="0" nbins="8" edges="225,400,500,600,700,850,1000,1250,2500" particle="tt" /> 
 	    <Bin id="1" nbins="50" xmin="225." xmax="2500." particle="tt" /> 
            <Bin id="0" nbins="50" xmin="0." xmax="200." particle="WL"/>
            <Bin id="0" nbins="50" xmin="0." xmax="200." particle="WH"/>
        </Variable>
        <Variable name="absrap" title="abs rapidity |y_{t}|" folder="4j2b" > 
          <Bin id="0" nbins="6" edges="0,0.4,0.8,1.1,1.4,1.8,2.5" particle="top"/>
          <Bin id="1" nbins="50" xmin="0." xmax="2.5" particle="top"/>
	  <Bin id="0" nbins="10" edges="0,0.1,0.2,0.4,0.6,0.7,0.8,1.0,1.2,1.6,2.5" particle="tt"/>
          <Bin id="1" nbins="25" xmin="0." xmax="2.5" particle="tt"/>
	  <Bin id="0" nbins="25" xmin="0." xmax="2.5" particle="WL"/>
	  <Bin id="0" nbins="25" xmin="0." xmax="2.5" particle="WH"/>
        </Variable>
        <Variable name="rapidity" title="rapidity y_{t}" folder="4j2b" > 
            <Bin id="0" nbins="12" edges="-2.5,-1.8,-1.4,-1.1,-0.8,-0.4,0.,0.4,0.8,1.1, 1.4,1.8,2.5" particle="top"/>
	    <Bin id="0" nbins="20" edges="-2.5,-1.6,-1.2,-1.0,-0.8,-0.7,-0.6,-0.4,-0.2,-0.1,0.,0.1,0.2,0.4,0.6,0.7,0.8,1.0,1.2,1.6,2.5" particle="tt"/>
  	    <Bin id="1" nbins="50" xmin="-2.5" xmax="2.5" particle="top"/>
	    <Bin id="1" nbins="50" xmin="-2.5" xmax="2.5" particle="tt"/>
  	    <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" particle="WL"/>
	    <Bin id="0" nbins="50" xmin="-2.5" xmax="2.5" particle="WH"/>
        </Variable>
        <Variable name="mlb" title="Reco (b,l) invariant mass [GeV]" folder="4j2b" >
            <Bin nbins="50" xmin="0." xmax="300." />
        </Variable>
        <Variable name="mu" title="Avg. no. of interactions per b.c." folder="cutflow">
            <Bin id="0" nbins="10" xmin="-0.5" xmax="49.5" />
            <Bin id="1" nbins="50" xmin="-0.5" xmax="49.5" />
        </Variable>
        <Variable name="pvxp_n" title="No. of primary vertices" folder="cutflow">
            <Bin id="0" nbins="10" xmin="0." xmax="20." />
        </Variable>
        <Variable name="vxp_z"	title="Primary vertex p_{z}" folder="cutflow">
            <Bin id="0" nbins="20" xmin="-300." xmax="300." />
       	</Variable>
        <Variable name="lep_pt"  title="Lepton p_{T} [GeV]" folder="cutflow"> 
	    <Bin id="0" nbins="14" edges="25.0,30.0,35.0,40.0,45.0,50.0,55.0,60.0,65.0,70.0,80.0,90.0,100.0,125.0,500.0" />
            <Bin id="1" nbins="100" xmin="0."   xmax="500." />
            <Bin id="2" nbins="40" xmin="0." xmax="200" /> <!-- ATL-COM-PHYS-2013-1100 -->
        </Variable>
        <Variable name="lep_eta" title="Lepton pseudorapidity #eta" folder="cutflow">
            <Bin id="0" nbins="25" xmin="-2.5"  xmax="2.5" />
        </Variable>        
        <Variable name="lep_phi" title="Lepton azimuthal angle #phi" folder="cutflow">
            <Bin id="0" nbins="25" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="lep_E" title="Lepton energy [GeV]" folder="cutflow">
            <Bin id="0" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet_n"   title="No. of Jets" folder="cutflow">
            <Bin  nbins="15" xmin="-0.5" xmax="14.5" />
        </Variable>
        <Variable name="jet_pt"  title="Jet p_{T} [GeV]" folder="cutflow">
	    <Bin id="0" nbins="19" edges="25.0,27.5,30.0,35.0,37.5,40.0,42.5,47.5,50.0,55.0,60.0,65.0,70.0,75.0,82.5,92.5,105.0,122.5,155.0,500.0" />
            <Bin id="1" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet1_pt"  title="Leading Jet p_{T} [GeV]" folder="cutflow">
            <Bin id="0" nbins="19" edges="25.0,27.5,30.0,35.0,37.5,40.0,42.5,47.5,50.0,55.0,60.0,65.0,70.0,75.0,82.5,92.5,105.0,122.5,155.0,500.0" />
            <Bin id="1" nbins="100" xmin="0."   xmax="500." />
            <Bin id="2" nbins="30" xmin="0." xmax="300." /> <!-- ATL-COM-PHYS-2013-1100 -->
        </Variable>
        <Variable name="jet_eta" title="Jet pseudorapidity #eta" folder="cutflow">
            <Bin nbins="50" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="jet_phi" title="Jet azimuthal angle #phi" folder="cutflow">
            <Bin nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="jet_E"   title="Jet energy [GeV]" folder="cutflow">
            <Bin nbins="200" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="jet_m"   title="Jet mass [GeV]" folder="cutflow">
            <Bin nbins="20" xmin="0."   xmax="100." />
        </Variable>
        <Variable name="bjet_n"   title="No. of b-Tagged Jets" folder="cutflow">
            <Bin nbins="5" xmin="-0.5" xmax="4.5" />
        </Variable>
        <Variable name="bjet_pt"  title="b-Jet p_{T} [GeV]" folder="cutflow">
            <Bin id="0" nbins="19" edges="25.0,27.5,30.0,35.0,37.5,40.0,42.5,47.5,50.0,55.0,60.0,65.0,70.0,75.0,82.5,92.5,105.0,122.5,155.0,500.0" />
            <Bin id="1" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="bjet1_pt"  title="Leading b-Jet p_{T} [GeV]" folder="cutflow">
            <Bin id="0" nbins="19" edges="25.0,27.5,30.0,35.0,37.5,40.0,42.5,47.5,50.0,55.0,60.0,65.0,70.0,75.0,82.5,92.5,105.0,122.5,155.0,500.0" />
            <Bin id="1" nbins="100" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="bjet_eta" title="b-Jet pseudorapidity #eta" folder="cutflow">
            <Bin nbins="50" xmin="-2.5"  xmax="2.5" />
        </Variable>
        <Variable name="bjet_phi" title="b-Jet azimuthal angle #phi" folder="cutflow">
            <Bin nbins="16" xmin="-3.1416"   xmax="3.1416" />
        </Variable>
        <Variable name="bjet_E"   title="b-Jet energy [GeV]" folder="cutflow">
            <Bin nbins="200" xmin="0."   xmax="500." />
        </Variable>
        <Variable name="bjet_m"   title="b-Jet mass [GeV]" folder="cutflow">
            <Bin nbins="20" xmin="0."   xmax="100." />
        </Variable>
        <Variable name="jet_ht"   title="Scalar sum of jet p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="20"  xmin="0." xmax="2000." />
            <Bin id="1" nbins="200" xmin="0." xmax="2000." />
            <Bin id="2" nbins="10" edges="0.00,150.00,170.00,190.00,210.00,230.00,260.00,290.00,340.00,420.00,2000.00" />
        </Variable>
        <Variable name="ht"   title="Scalar sum of final state objects p_{T} [GeV]" folder="cutflow" >
            <Bin id="0" nbins="20"  xmin="0." xmax="2000." />
            <Bin id="1" nbins="200" xmin="0." xmax="2000." />
            <Bin id="2" nbins="10" edges="0.00,250.00,280.00,310.00,330.00,360.00,390.00,430.00,490.00,580.00,2000.00" />
        </Variable>
         
        <Variable name="met_pt"  title="Missing Transverse Energy [GeV]"  folder="cutflow">
            <Bin id="0" nbins="20" edges="0,32,36,40,44,46,50,54,56,60,64,68,72,78,84,90,98,108,120,142,200" />
            <Bin id="1" nbins="100" xmin="0." xmax="200." />
            <Bin id="2" nbins="40" xmin="0." xmax="200." /> <!-- ATL-COM-PHYS-2013-1100 -->
        </Variable>
        <Variable name="mtw"     title="W boson Transverse Mass m_{T}^{W} [GeV]"  folder="cutflow">
            <Bin id="0" nbins="20" edges="0,40,46,50,56,60,62,66,70,72,76,78,82,86,88,92,98,104,112,128,200" />
            <Bin id="1" nbins="100" xmin="0" xmax="200" />
            <Bin id="2" nbins="40" xmin="0." xmax="200." /> <!-- ATL-COM-PHYS-2013-1100	-->
        </Variable>
        <Variable name="meff"   title="Effective mass [GeV]" folder="cutflow" >
            <Bin id="0" nbins="20"  xmin="0." xmax="2000." />
     	    <Bin id="1" nbins="200" xmin="0." xmax="2000." />
            <Bin id="2" nbins="10" edges="0.00,250.00,280.00,300.00,330.00,350.00,380.00,420.00,470.00,550.00,2000.00" />
      	</Variable>

        <Variable name="delta_R"  title="Matching #Delta R " folder="matching">
            <Bin nbins="20" xmin="0" xmax="5." />
        </Variable>
        <Variable name="delta_pt" title="Matching #Delta p_{T}" folder="matching">
            <Bin nbins="20" xmin="-200." xmax="200." />
        </Variable>
        <Variable name="delta_rapidity" title="Matching #Delta Y" folder="matching">
            <Bin nbins="20" xmin="-2." xmax="2." />
        </Variable>
        <Variable name="delta_phi" title="Matching #Delta #phi" folder="matching">
            <Bin nbins="20" xmin="-2." xmax="2." />
        </Variable>
        <Variable name="delta_m" title="Matching #Delta m" folder="matching">
            <Bin nbins="20" xmin="-200." xmax="200." />
        </Variable>


	<!-- top pair distributions -->
        <Variable name="z_ttbar" title="z_{t#bar{t}}" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="0.00,0.60,0.75,0.84,0.90,0.99,1.05,1.14,1.26,1.50,3.00" />
            <Bin id="1" nbins="100" xmin="0." xmax="3." />
        </Variable>
        <Variable name="Pout" title="p_{out}" folder="4j2b" particle="difference" >
	    <Bin id="0" nbins="10" edges="-300.00,-50.00,-24.00,-12.00,-6.00,0.0,6.00,12.00,24.00,50.00,300.00" />
            <Bin id="1" nbins="100" xmin="-300." xmax="300." />
        </Variable>
        <Variable name="Chi_ttbar" title="#chi_{out}" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" xmin="0." xmax="20." />
            <Bin id="1" nbins="100" xmin="0." xmax="20." />
        </Variable>
	<Variable name="Yboost" title="y_{boost}" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="7" edges="0.0,0.2,0.4,0.6,0.8,1.1,1.40,2." />
            <Bin id="1" nbins="100" xmin="0." xmax="2." />
        </Variable>
	<Variable name="topLCosThetaStar" title="leptonic top cos #theta^{*};cos #theta*;" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="-1.00,-0.82,-0.62,-0.42,-0.22,0.00,0.22,0.42,0.62,0.82,1.00" />
            <Bin id="1" nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="topHCosThetaStar" title="hadronic top cos #theta^{*};cos #theta*;" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="-1.00,-0.82,-0.62,-0.42,-0.22,0.00,0.22,0.42,0.62,0.82,1.00" />
            <Bin id="1" nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="TopCosOpeningAngle" title="Tops cos #theta_{12};Top's cos #theta_{12};" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="-1.00,-0.98,-0.90,-0.78,-0.60,-0.36,-0.08,0.20,0.50,0.76,1.00" />
            <Bin id="1" nbins="100" xmin="-1." xmax="1." />
        </Variable>
	<Variable name="Salam_ttbar" title="SalamDelta;[3*p_{T}^{(1)}-p_{T}^{(2)}] / H_{T}" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="-1.00,0.52,0.72,0.84,0.92,1.00,1.04,1.12,1.24,1.44,3.00" />
            <Bin id="1" nbins="100" xmin="-1." xmax="3." />
        </Variable>
	<Variable name="dPhi_ttbar" title="DeltaPhi_ttbar;#Delta#phi_{t#bar{t}}" folder="4j2b" particle="difference" >
            <Bin id="0" nbins="10" edges="0.00,2.29,2.61,2.76,2.86,2.92,2.98,3.02,3.05,3.08,3.1416" />
            <Bin id="1" nbins="100" xmin="0." xmax="3.1416" />
        </Variable>
        <Variable name="AverTopPt" title="Average p_{T}^{t} [GeV]" folder="4j2b" particle="difference" >
  	    <Bin id="0" nbins="10" edges="0,45,90,120,150,180,220,260,300,350,1000" />
            <Bin id="1" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="TopLPt" title="Average p_{T}^{t} [GeV]" folder="4j2b" particle="difference" >
  	    <Bin id="0" nbins="10" edges="0,45,90,120,150,180,220,260,300,350,1000" />
            <Bin id="1" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="TopHPt" title="Average p_{T}^{t} [GeV]" folder="4j2b" particle="difference" >
  	    <Bin id="0" nbins="10" edges="0,45,90,120,150,180,220,260,300,350,1000" />
            <Bin id="1" nbins="50" xmin="0" xmax="1000" />
        </Variable>
        <Variable name="dTopPt" title="Difference in p_{T}^{t} [GeV]" folder="4j2b" particle="difference" >
  	    <Bin id="0" nbins="18" edges="-600,-400,-200,-100,-80,-60,-40,-20,-10,0,10,20,40,60,80,100,200,400,600" />
            <Bin id="1" nbins="100" xmin="-600" xmax="600" />
        </Variable>
        <Variable name="dTopPtLH" title="Difference in p_{T}^{t} [GeV]" folder="4j2b" particle="difference" >
  	    <Bin id="0" nbins="18" edges="-600,-400,-200,-100,-80,-60,-40,-20,-10,0,10,20,40,60,80,100,200,400,600" />
            <Bin id="1" nbins="100" xmin="-600" xmax="600" />
        </Variable>

  </Variables>        


  <Histos>

        <!--TH2F variableX="pt" binIdX="0" variableY="lep_pt" binIdY="0" /-->
        <TH2F variableX="dPhi_ttbar" binIdX="0" variableY="Salam_ttbar" binIdY="0" folder="4j2b" particle="difference" /> 
        <TH2F variableX="dPhi_ttbar" binIdX="1" variableY="Salam_ttbar" binIdY="1" folder="4j2b" particle="difference" /> 
	<TH2F variableX="AverTopPt" binIdX="0" variableY="dTopPt" binIdY="0" folder="4j2b" particle="difference" /> 
	<TH2F variableX="AverTopPt" binIdX="1" variableY="dTopPt" binIdY="1" folder="4j2b" particle="difference" /> 
	<TH2F variableX="AverTopPt" binIdX="0" variableY="dTopPtLH" binIdY="0" folder="4j2b" particle="difference" /> 
	<TH2F variableX="AverTopPt" binIdX="1" variableY="dTopPtLH" binIdY="1" folder="4j2b" particle="difference" /> 
	<TH2F variableX="TopLPt" binIdX="1" variableY="dTopPtLH" binIdY="1" folder="4j2b" particle="difference" /> 
	<TH2F variableX="TopHPt" binIdX="1" variableY="dTopPtLH" binIdY="1" folder="4j2b" particle="difference" /> 

        <TH1F variable="ptdiff"/>
        <TH1F variable="pt" />
        <TH1F variable="eta" binId="0"/>
        <TH1F variable="phi" binId="0"/>
        <TH1F variable="E" binId="0"/>
        <TH1F variable="m" binId="0"/>
        <TH1F variable="absrap" binId="0"/>
        <TH1F variable="rapidity" binId="0"/>
        <TH1F variable="mlb" binId="0"/>
        <TH1F variable="mu" binId="0"/>
        <TH1F variable="pvxp_n" binId="0"/>
        <TH1F variable="vxp_z" binId="0"/>
        <TH1F variable="lep_pt" binId="1"/>
        <TH1F variable="lep_eta" binId="0"/>
        <TH1F variable="lep_phi" binId="0"/>                
        <TH1F variable="lep_E" binId="0"/>
        <TH1F variable="jet_n" binId="0"/>
        <TH1F variable="jet_pt" binId="1"/>
        <TH1F variable="jet_eta" binId="0"/>
        <TH1F variable="jet_phi" binId="0"/>                
        <TH1F variable="jet_E" binId="0"/>
        <TH1F variable="jet_m" binId="0"/>
        <TH1F variable="jet1_pt" binId="0"/>
        <TH1F variable="jet1_pt" binId="1"/>
        <TH1F variable="bjet_n" binId="0"/>
        <TH1F variable="bjet_pt" binId="0"/>
        <TH1F variable="bjet_eta" binId="0"/>
        <TH1F variable="bjet_phi" binId="0"/>                
        <TH1F variable="bjet_E" binId="0"/>
        <TH1F variable="bjet_m" binId="0"/>
        <TH1F variable="bjet1_pt" binId="0"/>
        <TH1F variable="bjet1_pt" binId="1"/>
        <TH1F variable="jet_ht" binId="0"/>
        <TH1F variable="jet_ht"	binId="1"/>
        <TH1F variable="met_pt" binId="1"/>
        <TH1F variable="mtw" binId="1"/>
        <TH1F variable="meff" binId="0" />
        <TH1F variable="meff" binId="1" />
        <TH1F variable="ht" binId="0" />
        <TH1F variable="ht" binId="1" />
        <TH1F variable="delta_R" binId="0"/>
        <TH1F variable="delta_pt" binId="0"/>
        <TH1F variable="delta_rapidity" binId="0"/>
        <TH1F variable="delta_phi" binId="0"/>
        <TH1F variable="delta_m" binId="0"/>

        <Matrix variable="pt" binId="0"/>
        <Matrix variable="eta" binId="0"/>
        <Matrix variable="phi" binId="0"/>
        <Matrix variable="E" binId="0"/>
        <Matrix variable="m" binId="0"/>
        <Matrix variable="absrap" binId="0"/>
        <Matrix variable="rapidity" binId="0"/>

 	<!-- top pair distributions -->
        <TH1F variable="z_ttbar" binId="0"/>
        <TH1F variable="Pout"  binId="0"/>
        <TH1F variable="Chi_ttbar"  binId="0"/>
	<TH1F variable="Yboost"  binId="0"/>
	<TH1F variable="topLCosThetaStar"  binId="0"/>
	<TH1F variable="topHCosThetaStar"  binId="0"/>
	<TH1F variable="TopCosOpeningAngle"  binId="0"/>
        <TH1F variable="Salam_ttbar"  binId="0"/>
	<TH1F variable="dPhi_ttbar"  binId="0"/>


    </Histos>    
    
  <!--TH1F name="vxp_nTracks"   title="No. of tracks per pv" nbins="20"  xmin="0" xmax="20" /-->
</histograms>
