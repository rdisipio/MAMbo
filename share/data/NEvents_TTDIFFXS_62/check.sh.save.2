#!/bin/bash
# set -x
ls *evt.n  | while read a
do 
  run=`echo $a | cut -d. -f1`
  
 # echo $run
  tag=`echo $a | cut -d. -f3`
  oldfile="ls `echo $a | sed s/TTDIFFXS_62_v5/TTDIFFXS_55_v2`
 # echo $a $oldfile
 nlines=`echo $oldfile | wc -l`
 echo $nlines
 if [ ! -e $oldfile ]
  then 
       continue
 fi
# cat $a
# cat $oldfile
  newevents=`cat $a | sed s/$run//g`
  oldevents=`cat $oldfile | sed s/$run//g`
  echo "$run : $newevents  $oldevents"
 done

 
